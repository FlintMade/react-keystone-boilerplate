{
  "name": "react-keystone-boilerplate",
  "description": "Example of a universal, hot-reloading web app using React, Redux, and Keystone CMS",
  "author": "Nate Bailey at Flint <nate@flintmade.co> (http://github.com/flintmade)",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlintMade/react-keystone-boilerplate"
  },
  "homepage": "https://github.com/FlintMade/react-keystone-boilerplate",
  "keywords": [
    "react",
    "keystone",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "npm run start-prod",
    "start-prod": "better-npm-run start-prod",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "postinstall": "webpack --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc source api",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "karma start"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./source",
        "NODE_ENV": "production",
        "COOKIE_SECRET": "production_cookies_yo"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./source",
        "NODE_ENV": "development",
        "PORT": 3000,
        "COOKIE_SECRET": "development_cookies_yo"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./source"
      }
    }
  },
  "dependencies": {
    "babel": "5.8.23",
    "babel-plugin-typecheck": "1.3.0",
    "body-parser": "1.14.1",
    "compression": "1.6.0",
    "cookie-parser": "^1.4.0",
    "express": "4.13.3",
    "express-session": "1.11.3",
    "file-loader": "0.8.4",
    "history": "1.12.5",
    "http-proxy": "1.11.3",
    "jsonp": "^0.2.0",
    "keystone": "^0.3.15",
    "less": "2.5.3",
    "less-loader": "2.2.1",
    "lru-memoize": "1.0.0",
    "map-props": "1.0.0",
    "multireducer": "1.0.2",
    "piping": "0.3.0",
    "platform": "^1.3.0",
    "pretty-error": "1.2.0",
    "query-string": "3.0.0",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.0",
    "react-document-meta": "2.0.0",
    "react-dom": "^0.14.3",
    "react-inline-css": "2.0.0",
    "react-mixin": "^2.0.2",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0-rc3",
    "redux": "3.0.3",
    "redux-form": "^3.0.10",
    "restful-keystone": "^0.3.0",
    "serialize-javascript": "1.1.2",
    "serve-favicon": "2.3.0",
    "serve-static": "1.10.0",
    "superagent": "^1.4.0",
    "underscore": "^1.8.3",
    "url-loader": "0.5.6",
    "webpack-isomorphic-tools": "1.0.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "better-npm-run": "^0.0.4",
    "bootstrap-sass": "^3.3.5",
    "bootstrap-sass-loader": "^1.0.9",
    "chai": "^3.3.0",
    "clean-webpack-plugin": "^0.1.3",
    "concurrently": "^0.1.1",
    "css-loader": "^0.20.1",
    "eslint": "1.7.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
